    "## 6. Tile Generation Preview\n",
    "\n",
    "Demonstrate how this data would be processed for tile generation."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Simulate tile generation process\n",
    "def calculate_tile_coverage(gdf, zoom_level):\n",
    "    \"\"\"Calculate which tiles would be needed to cover the data.\"\"\"\n",
    "    import math\n",
    "    \n",
    "    def deg2num(lat_deg, lon_deg, zoom):\n",
    "        \"\"\"Convert lat/lon to tile numbers.\"\"\"\n",
    "        lat_rad = math.radians(lat_deg)\n",
    "        n = 2.0 ** zoom\n",
    "        xtile = int((lon_deg + 180.0) / 360.0 * n)\n",
    "        ytile = int((1.0 - math.asinh(math.tan(lat_rad)) / math.pi) / 2.0 * n)\n",
    "        return (xtile, ytile)\n",
    "    \n",
    "    bounds = gdf.total_bounds\n",
    "    \n",
    "    # Calculate tile coordinates for corners\n",
    "    min_x, min_y = deg2num(bounds[3], bounds[0], zoom_level)  # NW corner\n",
    "    max_x, max_y = deg2num(bounds[1], bounds[2], zoom_level)  # SE corner\n",
    "    \n",
    "    tiles_needed = []\n",
    "    for x in range(min_x, max_x + 1):\n",
    "        for y in range(min_y, max_y + 1):\n",
    "            tiles_needed.append((zoom_level, x, y))\n",
    "    \n",
    "    return tiles_needed\n",
    "\n",
    "# Calculate tile coverage for different zoom levels\n",
    "zoom_levels = [10, 12, 14, 16]\n",
    "tile_counts = {}\n",
    "\n",
    "for zoom in zoom_levels:\n",
    "    tiles = calculate_tile_coverage(pois_gdf, zoom)\n",
    "    tile_counts[zoom] = len(tiles)\n",
    "    print(f\"Zoom {zoom}: {len(tiles)} tiles needed\")\n",
    "\n",
    "# Visualize tile count by zoom level\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.bar(tile_counts.keys(), tile_counts.values(), alpha=0.7)\n",
    "plt.xlabel('Zoom Level')\n",
    "plt.ylabel('Number of Tiles')\n",
    "plt.title('Tile Count by Zoom Level')\n",
    "plt.yscale('log')\n",
    "for zoom, count in tile_counts.items():\n",
    "    plt.text(zoom, count, str(count), ha='center', va='bottom')\n",
    "plt.show()\n",
    "\n",
    "print(f\"\\nTotal tiles across all zoom levels: {sum(tile_counts.values())}\")"
   ]
