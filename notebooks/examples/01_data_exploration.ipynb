    "## 5. Spatial Analysis\n",
    "\n",
    "Perform some basic spatial analysis on the data."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Calculate spatial statistics\n",
    "print(\"Spatial Analysis Results:\")\n",
    "print(\"=\" * 50)\n",
    "\n",
    "# Bounding box\n",
    "bounds = pois_gdf.total_bounds\n",
    "print(f\"Data Bounding Box:\")\n",
    "print(f\"  West: {bounds[0]:.4f}째\")\n",
    "print(f\"  South: {bounds[1]:.4f}째\")\n",
    "print(f\"  East: {bounds[2]:.4f}째\")\n",
    "print(f\"  North: {bounds[3]:.4f}째\")\n",
    "\n",
    "# Calculate distances between POIs\n",
    "pois_utm = pois_gdf.to_crs(epsg=32618)  # UTM Zone 18N for NYC\n",
    "\n",
    "distances = []\n",
    "for i in range(len(pois_utm)):\n",
    "    for j in range(i+1, len(pois_utm)):\n",
    "        dist = pois_utm.iloc[i].geometry.distance(pois_utm.iloc[j].geometry)\n",
    "        distances.append(dist)\n",
    "\n",
    "print(f\"\\nDistance Statistics (meters):\")\n",
    "print(f\"  Mean distance between POIs: {np.mean(distances):.0f}m\")\n",
    "print(f\"  Min distance: {np.min(distances):.0f}m\")\n",
    "print(f\"  Max distance: {np.max(distances):.0f}m\")\n",
    "\n",
    "# Road length analysis\n",
    "roads_utm = roads_gdf.to_crs(epsg=32618)\n",
    "road_lengths = roads_utm.geometry.length\n",
    "\n",
    "print(f\"\\nRoad Statistics:\")\n",
    "print(f\"  Total road length: {road_lengths.sum():.0f}m\")\n",
    "print(f\"  Average road segment length: {road_lengths.mean():.0f}m\")\n",
    "print(f\"  Longest road segment: {road_lengths.max():.0f}m\")\n",
    "print(f\"  Shortest road segment: {road_lengths.min():.0f}m\")"
